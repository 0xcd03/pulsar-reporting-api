		</property>
		<property name="tableName" value="session" />
		<property name="druidFilter"><null/></property>
		<property name="druidHaving"><null/></property>
		<property name="druidAggregators">
			<list>
				<ref bean="aggrSessions"/>
				<ref bean="aggrTotalPages"/>
