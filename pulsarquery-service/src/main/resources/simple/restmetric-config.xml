	<bean id="pageCtZero" class="com.ebay.pulsar.analytics.metricstore.druid.filter.SelectorFilter">
		<constructor-arg type="java.lang.String" value="pagect"/>
		<constructor-arg type="java.lang.String" value="0"/>
